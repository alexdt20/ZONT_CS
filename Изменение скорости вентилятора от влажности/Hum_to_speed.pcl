proc mainHS {} {
speed_humidity "Влажность в бассейне" "Скорость вентилятора"
}

proc speed_humidity {nh ns} {
set h [objstate $nh 0 0];
set s [min [lindex [split [expr "($h-50)/7+ 1"] "."] 0] 7];
sendmess sel [conjoin "" "#Y" [objid $ns 10] "=2,$s"];
;# objcmd $ns 2,$s 10;
puts "$ns $s"
}


setev chg mainHS@"Влажность в бассейне"
settim mainHS 150000 1
